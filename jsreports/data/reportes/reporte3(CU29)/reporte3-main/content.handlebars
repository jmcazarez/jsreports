{{!-- Reporte 3 --}}

{{#xlsxReplace "xl/worksheets/sheet1.xml" "worksheet.mergeCells"}}
    <mergeCells>
        <mergeCell ref="B1:B3"/>
        <mergeCell ref="C1:H1"/>
        <mergeCell ref="C2:H2"/>
        <mergeCell ref="C3:H3"/>
    </mergeCells>
{{/xlsxReplace}}

{{#xlsxReplace "xl/worksheets/sheet1.xml" "worksheet.sheetData[0].row[1]"}}
    <row>
        <c r="C2" t="inlineStr"><is><t>Información actualizada al día {{now}} </t></is></c>
    </row>
{{/xlsxReplace}}

{{#xlsxReplace "xl/worksheets/sheet1.xml" "worksheet.sheetData[0].row[2]"}}
    <row>
        <c r="C3" t="inlineStr"><is><t>{{cDireccion}}</t></is></c>
    </row>
{{/xlsxReplace}}

{{#xlsxReplace "xl/worksheets/sheet1.xml" "worksheet.sheetData[0].row[3]"}}
    <row>
        <c r="C4" t="inlineStr"><is><t>{{cEmail}}</t></is></c>
    </row>
{{/xlsxReplace}}


{{!-- Tabla-Header --}}

{{#xlsxAdd "xl/worksheets/sheet1.xml" "worksheet.sheetData[0].row"}}
    <row>
        <c></c>
        <c t="inlineStr"><is><t>Tipo</t></is></c>
        <c t="inlineStr"><is><t>Nombre Empresa</t></is></c>
        <c t="inlineStr"><is><t>RFC/Taxid</t></is></c>
        <c t="inlineStr"><is><t>Origen</t></is></c>
        <c t="inlineStr"><is><t>Domicilio</t></is></c>
        <c t="inlineStr"><is><t>Teléfono</t></is></c>
        <c t="inlineStr"><is><t>Correo</t></is></c>
        <c t="inlineStr"><is><t>Numero de Consultas</t></is></c>
    </row>
{{/xlsxAdd}}

{{!-- Vacio  --}}
{{#xlsxAdd "xl/worksheets/sheet1.xml" "worksheet.sheetData[0].row"}}
    <row> </row>
{{/xlsxAdd}}

{{!-- Tabla-Lista --}}
{{#each items}}
    {{#xlsxAdd "xl/worksheets/sheet1.xml" "worksheet.sheetData[0].row"}}
    <row>
        <c></c>
        <c t="inlineStr"><is><t>{{cTipo}}</t></is></c>
        <c t="inlineStr"><is><t>{{cEmpresa}}</t></is></c>
        <c t="inlineStr"><is><t>{{cRegistroIdentificador}}</t></is></c>
        <c t="inlineStr"><is><t>{{cTipoEmpresa}}</t></is></c>
        <c t="inlineStr"><is><t>{{cDireccionSucursalMatriz}}</t></is></c>
        <c t="inlineStr"><is><t>{{cTelefonoOficinaMatriz}}</t></is></c>
        <c t="inlineStr"><is><t>{{cEmail}}</t></is></c>
        <c t="inlineStr"><is><t>{{nCantidadConsultas}}</t></is></c>
    </row>
    {{/xlsxAdd}}
{{/each}}

{{!-- Vacio  --}}
{{#xlsxAdd "xl/worksheets/sheet1.xml" "worksheet.sheetData[0].row"}}
    <row> </row>
{{/xlsxAdd}}

{{!-- Total Consultas--}}
{{#xlsxAdd "xl/worksheets/sheet1.xml" "worksheet.sheetData[0].row"}}
    <row>
        <c></c>
        <c t="inlineStr"><is><t>Total</t></is></c>
        <c></c>
        <c></c>
        <c></c>
        <c></c>
        <c></c>
        <c></c>
        <c t="inlineStr"><is><t>{{nTotalConsultas}}</t></is></c>        
    </row>
{{/xlsxAdd}}

{{{xlsxPrint}}}